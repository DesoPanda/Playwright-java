<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression Suite" thread-count="3" parallel="tests" verbose="4">

	 <listeners>
		<listener class-name="j4.qa.opencart.Listners.ExtentReportListener" />

	</listeners>

	<test name="Open Cart HomePage Test">
		<classes>
			<class name="j4.qa.opencart.Test.HomePageTest" />
		</classes>
	</test>

	<test name="Open Cart RegisterPage Test">
		<classes>
			<class name="j4.qa.opencart.Test.RegisterPageTest"></class>
		</classes>
	</test>

	<test name="Open Cart SuccessPage Test">
		<classes>
			<class name="j4.qa.opencart.Test.SuccessPageTest"></class>
		</classes>
	</test>


